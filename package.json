{
  "name": "@cikeyqi/queqiao-node-sdk",
  "version": "0.1.1",
  "description": "QueQiao Protocol V2 Node.js SDK",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && node --test --test-concurrency=1 --test-isolation=none",
    "test:strict": "npm run build && node --test",
    "check": "npm run lint && npm test",
    "lint": "eslint . --ext .ts",
    "format": "prettier -w .",
    "prepublishOnly": "npm run check"
  },
  "engines": {
    "node": ">=18.18"
  },
  "dependencies": {
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
